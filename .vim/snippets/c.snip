# 以下自分で追加又は上書きするもの
# デフォルトで利用可能なのは~/.vim/bundle/neocomplcache/autoload/neocomplcache/sources/snippets_complete/ 以下のファイルを参照

snippet scanf
    scanf("${1}", &${2});${0}

snippet main
    int main(int argc, char *argv[])
    {
    	${0}
    	return 0;
    }

snippet typedef_struct
    typedef struct ${1}{
    	${0:/* data */}
    } ${2:type_t};

