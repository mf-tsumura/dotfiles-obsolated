snippet     throws
abbr        throws
        throws ${0:Exception} 


snippet     class
abbr        class
        class ${1:ClassName} ${2}{
                ${0}
        }

snippet     interface
abbr        interface
        interface ${1:InterfaceName} {
                ${0}
        }


snippet     constant
abbr        constant value
prev_word   '^'
        public static final ${1:Type} ${2:CONST_VALUE} = ${3};${0}

snippet     return
abbr        return
prev_word   '^'
        return ${1};${0}

snippet     throw
abbr        throw Exception
prev_word   '^'
        throw new ${1:Exception()};


snippet     main
abbr        main
prev_word   '^'
